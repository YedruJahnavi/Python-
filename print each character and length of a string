input_string = input("enter a string:")
print("character in input_string:")
for character in input_string:
  print(character)
string_length = len(input_string)
print(f"the length of the string is: {string_length}")


OUTPUT:

input_string = input("enter a string:")
print("character in input_string:")
for character in input_string:
  print(character)
string_length = len(input_string)
print(f"the length of the string is: {string_length}")
